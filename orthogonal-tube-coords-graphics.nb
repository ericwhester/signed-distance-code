     RowBox[{"t", "[", "s", "]"}], "\[Cross]", 
     RowBox[{"n", "[", "s", "]"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Omega]", "[", "s_", "]"}], "=", 
   RowBox[{
    RowBox[{
     FractionBox["1", 
      RowBox[{"v", "[", "s", "]"}]], 
     RowBox[{
      RowBox[{
       SubscriptBox["\[PartialD]", "s"], 
       RowBox[{"(", 
        RowBox[{"n", "[", "s", "]"}], ")"}]}], ".", 
      RowBox[{"b", "[", "s", "]"}]}]}], "//", "Simplify"}]}], ";"}], "\n", 
 RowBox[{
