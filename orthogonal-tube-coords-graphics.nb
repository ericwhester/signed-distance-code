Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"p", "[", "s_", "]"}], ":=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Cos", "[", 
      RowBox[{"2", "\[Pi]", " ", "s"}], "]"}], ",", 
     RowBox[{"Sin", "[", 
      RowBox[{"2", " ", "\[Pi]", " ", "s"}], "]"}], ",", 
     SuperscriptBox["s", "2"]}], "}"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"t0", "[", "s_", "]"}], "=", 
   RowBox[{
    RowBox[{"p", "'"}], "[", "s", "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"v", "[", "s_", "]"}], "=", 
   SuperscriptBox[
    RowBox[{"Simplify", "[", 
     RowBox[{
      RowBox[{"t0", "[", "s", "]"}], ".", 
      RowBox[{"t0", "[", "s", "]"}]}], "]"}], 
    RowBox[{"1", "/", "2"}]]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"t", "[", "s_", "]"}], "=", 
   RowBox[{"Simplify", "[", 
    FractionBox[
     RowBox[{"t0", "[", "s", "]"}], 
     RowBox[{"v", "[", "s", "]"}]], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"n0", "[", "s_", "]"}], "=", 
   RowBox[{
    FractionBox["1", 
     RowBox[{"v", "[", "s", "]"}]], 
    RowBox[{
     SubscriptBox["\[PartialD]", "s"], 
     RowBox[{"(", 
      RowBox[{"t", "[", "s", "]"}], ")"}]}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Kappa]", "[", "s_", "]"}], "=", 
   SuperscriptBox[
    RowBox[{"FullSimplify", "[", 
     RowBox[{
      RowBox[{"n0", "[", "s", "]"}], ".", 
      RowBox[{"n0", "[", "s", "]"}]}], "]"}], 
    RowBox[{"1", "/", "2"}]]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"n", "[", "s_", "]"}], "=", 
   RowBox[{"Simplify", "[", 
